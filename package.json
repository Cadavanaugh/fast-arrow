{
    "name": "fast-arrow",
    "displayName": "Fast Arrow",
    "description": "Write fat arrow in less than 1 second",
    "version": "1.0.0",
    "icon": "icon.png",
    "repository": {
		"type": "git",
		"url": "https://github.com/vinliao/vscode-bcdn"
	},
    "publisher": "vinliao",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/snippets.json"
            },
            {
                "languages": [{
                    "id": "vue",
                    "extensions": [ ".vue" ],
                    "aliases": [ "Vue", "vue", "vue.js" ]
                }],
                "path": "./snippets/snippets.json"
            }
        ]
    }
}